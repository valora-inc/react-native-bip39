{
  "name": "react-native-bip39",
  "version": "2.2.0",
  "description": "ReactNative-ready Bitcoin BIP39: Mnemonic code for generating deterministic keys",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "scripts": {
    "standard": "standard"
  },
  "author": "Alberto Dallaporta",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/39otrebla/react-native-bip39"
  },
  "license": "MIT",
  "dependencies": {
    "create-hash": "^1.1.0",
    "@celo/react-native-fast-crypto": "^1.8.4",
    "react-native-securerandom": "1.0.0",
    "unorm": "^1.3.3"
  },
  "devDependencies": {
    "@react-native-community/bob": "^0.16.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "standard": "16.0.1",
    "tape": "^5.0.1",
    "typescript": "^4.0.5"
  },
  "react-native": "src/index.ts",
  "types": "lib/typescript/index.d.ts",
  "files": [
    ".",
    "index.ts",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "copyFlow": true
        }
      ],
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
